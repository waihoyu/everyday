*
 -webkit-tap-highlight-color transparent
 outline 0
 margin 0
 padding 0
 border 0


img 
 border 0 none 
 vertical-align middle

 ol,ul
  list-style  none
input, select, button, h1, h2, h3, h4, h5, h6
  font-size 100%
  font-family inherit 

table
  border-spacing 0
  border-collapse collapse
body
  margin 0 auto
  width 100%
  height 100%
  min-width 320px
  max-width 768px
  font-size 14px
  font-family -apple-system, Arial, Helvetica, sans-serif, "Microsoft YaHei"
  line-height 1.5
  color #666666
  -moz-text-size-adjust 100% !important 
  user-select none
input[type="text"],textarea
  appearance none    
html
 width 100%
 height 10%
.hide
  display none !important 
